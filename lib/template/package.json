{
  "name": "thinkkoa-application",
  "description": "application created by thinkkoa",
  "version": "1.0.0",
  "scripts": {
    "test": "npm run eslint",
    "start": "npm run compile && supervisor -w app -q index",
    "compile": "babel src -d app -D",
    "watch-compile": "npm run compile -- --watch",
    "eslint": "eslint src"
  },
  "babel": {
    "presets": [
      ["env", {"targets": {"node": true}, "loose": true}]
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "dependencies": {
    "babel-runtime": "6.23.0",
    "thinkkoa": "^1.x.x"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "eslint": "^3.19.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "supertest": "^3.0.0",
    "supervisor": "^0.12.0"
  }
}
